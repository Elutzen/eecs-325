(defun show-list (lst)
  (cond ((atom lst) (format t "~s" lst))
        (t (format t "[") 
           (show-list (car lst))
           (show-elements (cdr lst))
           (format t "]"))))
           
(defun show-elements (lst)
  (cond ((consp lst) (format t " ") 
         (show-list (car lst)) 
         (show-elements (cdr lst)))
        ((null lst) t)
        ((atom lst) (format t " . ~s" lst))
        (t)))
